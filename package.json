{
  "name": "react-client",
  "version": "1.0.0",
  "description": "A React client built with Next.js",
  "main": ".next/server/index.js",
  "repository": "https://github.com/meebix/react-client.git",
  "author": "Michael Donahue <mtdonahue1@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rm -rf .next && yarn build:client && yarn build:server",
    "build:client": "tsc -p tsconfig.client.json && next build",
    "build:server": "tsc -p tsconfig.server.json && webpack --config webpack.server.js",
    "start": "NODE_ENV=production node .next/server",
    "dev": "nodemon server/index.ts",
    "lint": "eslint . --ext '.ts,.tsx'",
    "prettier:fix": "prettier \"**/*.{ts,tsx}\" --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:deploy": "prisma deploy",
    "db:seed": "prisma seed",
    "db:seed:reset": "yarn db:seed --reset",
    "db:reset": "prisma reset",
    "db:admin": "prisma admin"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-http": "^1.5.14",
    "apollo-server-express": "^2.5.0",
    "argon2": "^0.22.0",
    "assign-deep": "^1.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.10.0",
    "date-fns": "^1.30.1",
    "deepmerge": "^3.2.0",
    "express": "^4.16.4",
    "express-healthcheck": "^0.1.0",
    "formik": "^1.5.7",
    "glob": "^7.1.4",
    "googleapis": "^40.0.0",
    "graphql": "^14.3.1",
    "graphql-middleware": "^3.0.2",
    "graphql-shield": "^5.3.8",
    "graphql-tag": "^2.10.1",
    "helmet": "^3.16.0",
    "is-glob": "^4.0.1",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^9.0.0",
    "next": "^8.1.0",
    "pino": "^5.12.3",
    "pino-pretty": "^3.0.0",
    "prisma": "^1.34.0",
    "prisma-client-lib": "^1.34.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "semantic-ui-react": "^0.86.0",
    "sparkpost": "^2.1.3",
    "stripe": "^6.34.0",
    "universal-cookie": "^4.0.0",
    "uuid": "^3.3.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "@types/config": "^0.0.34",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.5",
    "@types/csurf": "^1.9.35",
    "@types/express": "^4.17.0",
    "@types/helmet": "^0.0.43",
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.17",
    "@types/webpack-env": "^1.13.9",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "@zeit/next-less": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "babel-loader": "^8.0.6",
    "chance": "^1.0.18",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-typescript": "^3.0.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "husky": "^2.1.0",
    "less": "^3.9.0",
    "lint-staged": "^8.1.5",
    "nodemon": "^1.19.0",
    "prettier": "^1.16.4",
    "semantic-release": "^15.13.3",
    "semantic-ui-less": "^2.4.1",
    "ts-loader": "^6.0.0",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.4.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "concurrent": true,
    "linters": {
      "*.{ts,tsx}": [
        "yarn prettier:fix",
        "yarn lint",
        "git add"
      ]
    }
  }
}
